[alias]    	
    s = status   
	ss = status -s   
    a = !sh -c 'git add $1' -
    aa = add -A
    c = commit
    ca = commit --amend
    addcom = !sh -c 'git add -A && git commit -m $1' -
    save = !sh -c 'git add -A && git commit -m 'Work in progress''
    st = stash
    stl = stash list
    sta = stash apply
    stp = stash pop
    stb = !sh -c 'git stash branch $1' -
    sts = !sh -c 'git stash show -p stash@{$1}' -
    dev = checkout develop
    mas = checkout master
    co = !sh -c 'git checkout $1' -
    cob = !sh -c 'git checkout -b $1 ' -
    mer = !sh -c 'git merge --no-ff $1 ' -
    res = reset
    resh = reset HEAD --hard
    rev = revert
    cln = clean -fdx
    branchcd = branch --sort=-committerdate
    branchcdr = for-each-ref --sort=-committerdate refs/heads/
    l = !sh -c 'git log --oneline --all --graph --decorate  $1' -
    ll = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
    dog = log --all --decorate --oneline --graph
    wdiff = diff --color-words
    gfl = log -u